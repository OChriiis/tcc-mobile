<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.towersadmin">

    <uses-sdk android:minSdkVersion="10" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_towers_logo_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_towers_logo_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TowersAdmin"
        android:usesCleartextTraffic="true"
        tools:targetApi="m">
        <activity
            android:name=".ui.ChatActivityLog"
            android:exported="false" />
        <activity android:name=".ui.CadastroVisitanteSindico" />
        <activity
            android:name=".ui.dashboards.DashBoardMorador"
            android:exported="false" />
        <activity android:name=".ui.aviso.EditarAviso" />
        <activity android:name=".ui.aviso.NovoAviso" />
        <activity android:name=".ui.AgendamentoEspaco" />
        <activity android:name=".ui.activity_votacao" />
        <activity android:name=".ui.activity_reclamacoes" />
        <activity android:name=".ui.aviso.activity_aviso" />
        <activity android:name=".ui.logins.LoginSindicoActivity" />
        <activity android:name=".ui.logins.LoginActivity" />
        <activity android:name=".ui.aviso.activity_aviso_morador"/>
        <activity android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.CadastroAdminActivity"
            android:parentActivityName=".ui.dashboards.DashBoardActivity" />
        <activity
            android:name=".ui.CadastroVisitanteActivity"
            android:parentActivityName=".ui.dashboards.DashBoardActivity" />
        <activity android:name=".ui.dashboards.DashBoardActivity" />
        <activity
            android:name=".ui.CadastroMoradorActivity"
            android:parentActivityName=".ui.dashboards.DashBoardActivity" />
    </application>

</manifest>